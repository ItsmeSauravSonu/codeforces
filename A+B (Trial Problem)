import java.util.Scanner;
 
public class trialproblem {
 
	public static void main(String[] args) {
		Scanner scn = new Scanner(System.in);
        int t = scn.nextInt();
        while(t-->0) {
        	int a = scn.nextInt();
        	int b = scn.nextInt();
        	int sum = a+b;
        	System.out.println(sum);
        }
        	
	}
 
}
